<div class="container">
    <div class="jumbotron">
        <h2>Create Account</h2>
        <form class="create-form">
            <div>
                <label for="email">Email:</label>
                <input type="text" id="email" name="emailField" placeholder="Enter your email...">
                <br>
                <label for="password">Password</label>
                <input type="password" id="password" name="passwordField" placeholder="Create a password...">
            </div>

            <button id="signUpSubmit" type="submit">Submit</button>
        </form>

    </div>

</div>